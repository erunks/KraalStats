{
  "name": "smashcoach",
  "version": "1.0.0",
  "engines": {
    "node": "11.0.0",
    "npm": "6.7.0"
  },
  "description": "A Discord bot for tracking match-up stats in smash",
  "main": "./app/app.js",
  "scripts": {
    "test": "mocha ./app/test/*.test.{js,ts}",
    "test:watch": "mocha ./app/test/*.test.{js,ts} --watch",
    "prestart": "knex migrate:latest --env production",
    "start": "nodemon --inspect ./app/app.js",
    "format:prettier": "prettier --config ./config/prettier.config.js --write '{app,assets,db}/**/*.js'",
    "lint": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erunks/SmashCoach.git"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Edward Runkel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erunks/SmashCoach/issues"
  },
  "homepage": "https://github.com/erunks/SmashCoach#readme",
  "devDependencies": {
    "bufferutil": "^3.0.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "erlpack": "github:discordapp/erlpack",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-json": "^1.4.0",
    "husky": "^1.3.1",
    "libsodium-wrappers": "^0.7.4",
    "lint-staged": "^8.1.5",
    "mocha": "^5.2.0",
    "node-opus": "^0.2.9",
    "opusscript": "0.0.6",
    "prettier": "^1.17.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "uws": "^9.148.0"
  },
  "dependencies": {
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
    "knex": "^0.16.5",
    "mysql": "^2.17.1",
    "nodemon": "^1.19.0",
    "npm": "^6.9.0",
    "objection": "^1.6.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "npm run lint:eslint -- .",
        "npm run format:prettier",
        "git add"
      ]
    }
  }
}
